def workspace
node
{
  
      stage('checkout')
    {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/reddyrepo997/maven-web-application.git']]])
        workspace=pwd()
    }
    
    stage('Build')
    {
        echo "Building the job"
    }
    stage('test')
    {
        echo "Testing the job"
    }
    stage('Deploy')
    {
        echo "Deployimg the code"
    }
}  
